olumn":12}},{"generated":{"line":115,"column":9},"source":"components/App.jsx","original":{"line":51,"column":8}},{"generated":{"line":116,"column":0},"source":"components/App.jsx","original":{"line":59,"column":5}},{"name":"Component","generated":{"line":120,"column":2},"source":"components/App.jsx","original":{"line":8,"column":18}},{"name":"App","generated":{"line":122,"column":15},"source":"components/App.jsx","original":{"line":62,"column":15}}],"sources":{"components/App.jsx":"import React, { Component } from 'react'\r\nimport axios from 'axios'\r\nimport GitHubCard from './GitHubCard'\r\n\r\nimport '../styles/styles.less'\r\nimport Axios from 'axios'\r\n\r\nclass App extends Component {\r\n    constructor() {\r\n        super()\r\n\r\n        this.state = {\r\n            user: {},\r\n            followers: []\r\n        }\r\n    }\r\n\r\n    componentDidMount() {\r\n        //Get my user info from API\r\n        axios\r\n            .get(`https://api.github.com/users/jessemarek`)\r\n            .then(res => {\r\n                this.setState({\r\n                    user: res.data\r\n                })\r\n            })\r\n            .catch(err => console.log('ERROR: ', err))\r\n\r\n        //Get all of my followers and set the data into state\r\n        axios.get(`https://api.github.com/users/jessemarek/followers`)\r\n            .then(res => {\r\n                res.data.forEach(item => {\r\n                    //request data for each follower\r\n                    axios.get(`${item.url}`)\r\n                        .then(res => {\r\n                            //Add the follower data to state\r\n                            this.setState({\r\n                                followers: [...this.state.followers, res.data]\r\n                            })\r\n                            \r\n                        })\r\n                        .catch(err => console.log('ERROR:', err))\r\n                })\r\n            })\r\n            .catch(err => {\r\n                console.log('Error: ', err)\r\n            })\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div>\r\n                <GitHubCard data={this.state.user} />\r\n                {\r\n                    this.state.followers.map(f => <GitHubCard key={f.id} data={f} />)\r\n                }\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default App"},"lineCount":null}},"error":null,"hash":"ce55f2450168ad6b47529380e45804dd","cacheData":{"env":{}}}